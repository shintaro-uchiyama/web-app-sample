{
    "Comment": "Send message from server on job finished",
    "StartAt": "LookupCustomerInfo",
    "States": {
      "LookupCustomerInfo": {
        "Type": "Parallel",
        "Next": "HelloWorld",
        "Branches": [
          {
           "StartAt": "ConnectToClient",
           "States": {
             "ConnectToClient": {
               "Type": "Task",
               "Resource":
                 "arn:aws:lambda:us-east-1:123456789012:function:AddressFinder",
               "End": true
             }
           }
         },
         {
           "StartAt": "FileImport",
           "States": {
             "FileImport": {
               "Type": "Task",
               "Resource":
                 "arn:aws:lambda:us-east-1:123456789012:function:PhoneFinder",
               "End": true
             }
           }
         }
        ]
      },
      "HelloWorld": {
        "Type": "Task",
        "Resource": "arn:aws:lambda:us-east-1:123456789012:function:HelloFunction",
        "End": true,
        "Comment": "Run the HelloWorld Lambda function"
      }
    }
}